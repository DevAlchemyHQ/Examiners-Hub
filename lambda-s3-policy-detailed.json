{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3ReadAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::mvp-labeler-storage/*"
      ]
    },
    {
      "Sid": "AllowS3WriteAccess",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::mvp-labeler-storage/downloads/*",
        "arn:aws:s3:::mvp-labeler-storage/users/*"
      ]
    },
    {
      "Sid": "AllowS3ListAccess",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::mvp-labeler-storage"
      ]
    }
  ]
} 