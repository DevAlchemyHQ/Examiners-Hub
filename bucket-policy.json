{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::mvp-labeler-storage/*"
    },
    {
      "Sid": "AllowUserOperations",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::913524945607:user/Exametry_migrate"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::mvp-labeler-storage",
        "arn:aws:s3:::mvp-labeler-storage/*"
      ]
    }
  ]
}
